default:\
        :tc=extensions:\
        :tc=builtin-parser@/usr/local/etc/gtags.conf:\
        :tc=pygments-parser@/usr/local/etc/gtags.conf:

extensions:\
        :skip=.idea,.lein-*,.nrepl-port,node_modules/,target/,vendor/,vendors/:\
        :ctagscom=/usr/local/opt/ctags/bin/ctags:\
        :pygmentslib=$libdir/gtags/pygments-parser.la:\
        :langmap=CSS\:.css.less.sass.scss:\
        :gtags_parser=CSS\:$pygmentslib:\
        :langmap=JS\:.js.jsx:\
        :gtags_parser=JS\:$pygmentslib:\
        :langmap=YAML\:.yml.yaml:\
        :gtags_parser=YAML\:$pygmentslib:
