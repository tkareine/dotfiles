default:\
        :tc=common@/usr/local/etc/gtags.conf:\
        :tc=extensions:\
        :tc=builtin-parser@/usr/local/etc/gtags.conf:\
        :tc=exuberant-ctags@/usr/local/etc/gtags.conf:

extensions:\
        :skip=.idea,.lein-*,.nrepl-port,node_modules/,target/,vendor/,vendors/:\
        :ctagscom=/usr/local/opt/ctags/bin/ctags:\
        :ctagslib=$libdir/gtags/exuberant-ctags.la:\
        :langmap=CSS\:.css.less.sass.scss:\
        :gtags_parser=CSS\:$ctagslib:\
        :langmap=JS\:.js.jsx:\
        :gtags_parser=JS\:$ctagslib:
