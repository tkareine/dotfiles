default:\
        :tc=extensions:\
        :tc=builtin-parser@~/brew/etc/gtags.conf:\
        :tc=pygments-parser@~/brew/etc/gtags.conf:

extensions:\
        :skip=.git,.idea,.lein-*,.nrepl-port,node_modules/,target/,vendor/,vendors/:\
        :pygmentslib=$libdir/gtags/pygments-parser.la:\
        :langmap=CSS\:.css.less.sass.scss:\
        :gtags_parser=CSS\:$pygmentslib:\
        :langmap=JS\:.js.jsx:\
        :gtags_parser=JS\:$pygmentslib:\
        :langmap=YAML\:.yml.yaml:\
        :gtags_parser=YAML\:$pygmentslib:
