# Test configuration in effect with `gtags --conf`

default:\
        :tc=skip:\
        :tc=libs:\
        :tc=builtin-parser:\
        :tc=pygments-parser:

skip:\
        :skip=.git/,gtags.files,.idea/,.lein-*,.nrepl-port,node_modules/,TAGS,tags,target/,vendor/,vendors/,*~,*.bak,*.bz2,*.gz,*.lzh,*.xz,*.Z,*.zip,*.tgz,*.min.css,*.min.js:

libs:\
        :ctagscom=/usr/local/opt/universal-ctags/bin/ctags:\
        :ctagslib=$libdir/gtags/universal-ctags.la:\
        :pygmentslib=$libdir/gtags/pygments-parser.la:

builtin-parser:\
        :langmap=c\:.c.h,yacc\:.y,asm\:.s.S,java\:.java,cpp\:.c++.cc.hh.cpp.cxx.hxx.hpp.C.H,php\:.php.php3.phtml:

pygments-parser:\
        :langmap=AppleScript\:.applescript:\
        :gtags_parser=AppleScript\:$pygmentslib:\
        :langmap=Awk\:.awk.gawk.mawk:\
        :gtags_parser=Awk\:$pygmentslib:\
        :langmap=Bash\:.sh.ksh.bash.ebuild.eclass:\
        :gtags_parser=Bash\:$pygmentslib:\
        :langmap=Bat\:.bat.cmd:\
        :gtags_parser=Bat\:$pygmentslib:\
        :langmap=C#\:.cs:\
        :gtags_parser=C#\:$pygmentslib:\
