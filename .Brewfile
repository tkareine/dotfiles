tap "d12frosted/emacs-plus"
tap "tkareine/chnode"

brew "awscli"
brew "azure-cli"
brew "bash"
brew "bash-completion@2"
brew "chruby"
brew "coreutils"
brew "d12frosted/emacs-plus/emacs-plus@30", args: %w[with-modern-icon] if OS.mac?
brew "ffmpeg"
brew "fzf"
brew "git"
brew "global"
brew "gnupg"
brew "jq"
brew "libressl"
brew "nmap"
brew "node@24"
brew "openssl@3"
brew "pinentry-mac" if OS.mac?
brew "pnpm"
brew "readline"
brew "ripgrep"
brew "ruby-install"
brew "shellcheck"
brew "shfmt"
brew "tkareine/chnode/chnode"
brew "tmux"
brew "yt-dlp"
brew "zoxide"

cask "alfred"
