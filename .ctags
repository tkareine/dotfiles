--exclude=*~
--exclude=.idea
--exclude=.lein-*
--exclude=.nrepl-port
--exclude=*.min.css
--exclude=*.min.js
--exclude=node_modules
--exclude=target
--exclude=vendor
--exclude=vendors

--langdef=css
--langmap=css:.css
--langmap=css:+.less
--langmap=css:+.sass
--langmap=css:+.scss
--regex-css=/^[ \t]*@mixin[ \t]+([A-Za-z_][A-Za-z0-9_-]+)[ \t]*(\([^)]+\)[ \t]*)?\{/\1/m,mixins/
--regex-css=/^[ \t]*([A-Za-z_][A-Za-z0-9_-]+)?#([A-Za-z_][A-Za-z0-9_-]+)[ \t]*[,{]/\2/i,ids/
--regex-css=/^[ \t]*([A-Za-z_][A-Za-z0-9_-]+)?\.([A-Za-z_][A-Za-z0-9_-]+)[ \t]*[,{]/\2/c,classes/
--regex-css=/^[ \t]*[\$@]([A-Za-z_][A-Za-z0-9_-]+):/\1/v,variables/

--languages=-javascript

--langdef=js
--langmap=js:.js
--langmap=js:+.jsx
--regex-js=/(^|[ \t]+)(async[ \t]+)?\(?function[ \t]*(\*[ \t]*)?([A-Za-z_$][A-Za-z0-9_$]+)[ \t\(]/\4/f,functions/
--regex-js=/(^|[ \t]+)([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*=[ \t]*(async[ \t]+)?function[ \t]*(\*[ \t]*)?\(/\2/f,functions/
--regex-js=/(^|[ \t]+)([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*=[ \t]*(\([^)]*\)|[^)]+)[ \t]*=>/\2/f,functions/
--regex-js=/(^|[ \t]+)([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*=[ \t]*null([;, \t]|$)/\2/n,nulls/
--regex-js=/(^|[ \t]+)([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*=[ \t]*(true|false)([;, \t]|$)/\2/b,booleans/
--regex-js=/(^|[ \t]+)([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*=[ \t]*[0-9]/\2/n,numbers/
--regex-js=/(^|[ \t]+)([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*=[ \t]*([A-Za-z_$][A-Za-z0-9_$]+)?('|"|`)/\2/s,strings/
--regex-js=/(^|[ \t]+)([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*=[ \t]*\//\2/r,regexes/
--regex-js=/(^|[ \t]+)([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*=[ \t]*\[/\2/a,arrays/
--regex-js=/(^|[ \t]+)([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*=[ \t]*\{/\2/o,objects/
--regex-js=/(^|[ \t]+)([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*=[ \t]*class[ \t]*\{/\2/c,classes/
--regex-js=/(^|[ \t]+)([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*=[ \t]*React.createClass[ \t]*\(/\2/c,classes/
--regex-js=/\.([A-Za-z_$][A-Za-z0-9._$]+)[ \t]*=/\1/f,fields/
--regex-js=/(^|[ \t]+|,)([A-Za-z_$][A-Za-z0-9._$]+)[ \t]*:/\2/f,fields/
--regex-js=/'([^']+)'[ \t]*:/\1/f,fields/
--regex-js=/"([^"]+)"[ \t]*:/\1/f,fields/
--regex-js=/(^|[ \t]+)class[ \t]+([A-Za-z_$][A-Za-z0-9._$]+)[ \t]*($|\{|extends)/\2/c,classes/
--regex-js=/^([ \t]*static)?([ \t]*async)?([ \t]*\*)?[ \t]*([A-Za-z_$][A-Za-z0-9_$]+)[ \t]*\([^)]*\)[ \t]*\{/\4/m,methods/

--regex-ruby=/([A-Z_]+[A-Z0-9_]+)[ \t]*=/\1/k,constants/

--langdef=yaml
--langmap=yaml:.yml
--langmap=yaml:+.yaml
--regex-yaml=/^[ \t]*([A-Za-z0-9.\-_$]+):/\1/m,mappings/
--regex-yaml=/^[ \t]*'([^']+)':/\1/m,mappings/
--regex-yaml=/^[ \t]*"([^"]+)":/\1/m,mappings/
--regex-yaml=/^[ \t]*register:[ \t]*([A-Za-z0-9_$]+)/\1/r,ansible registers/
--regex-yaml=/^[ \t]*['"]?register['"]?:[ \t]*([A-Za-z0-9_$]+)/\1/r,ansible registers/
--regex-yaml=/^[ \t]*['"]?register['"]?:[ \t]*'([^']+)'/\1/r,ansible registers/
--regex-yaml=/^[ \t]*['"]?register['"]?:[ \t]*"([^"]+)"/\1/r,ansible registers/
